{
    "name": "ornito/rest-create-users",
    "version": "1.1",
    "description": "Create users in Bolt CMS through API.",
    "type": "bolt-extension",
    "keywords": [
        "rest",
        "restful",
        "json",
        "authentication",
        "authorization",
        "jwt",
        "json web token"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Kurauchi",
            "email": "victorkurauchi@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Ornito\\RestCreateUser\\": "src"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Ornito\\RestCreateUser\\RestCreateUserExtension",
        "bolt-icon": "bolt-icon.png",
        "bolt-screenshots": [
            "screenshots/screenshot1.png"
        ]
    }
}
